{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/normative-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/normative-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/normative-backend",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "events": {
      "type": "library",
      "root": "libs/events",
      "entryFile": "index",
      "sourceRoot": "libs/events/src",
      "compilerOptions": {
        "tsConfigPath": "libs/events/tsconfig.lib.json"
      }
    },
    "form-service": {
      "type": "application",
      "root": "apps/form-service",
      "entryFile": "main",
      "sourceRoot": "apps/form-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/form-service/tsconfig.app.json"
      }
    },
    "normativas-service": {
      "type": "application",
      "root": "apps/normativas-service",
      "entryFile": "main",
      "sourceRoot": "apps/normativas-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/normativas-service/tsconfig.app.json"
      }
    },
    "normative-backend": {
      "type": "application",
      "root": "apps/normative-backend",
      "entryFile": "main",
      "sourceRoot": "apps/normative-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/normative-backend/tsconfig.app.json"
      }
    }
  }
}